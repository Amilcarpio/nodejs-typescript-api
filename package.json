{
  "name": "oztest",
  "version": "0.0.1",
  "description": "DevOZ - Tech test",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha -r ts-node/register \"src/**/*.spec.ts\" --exit",
    "test:ci": "nyc mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "build": "tsc --noEmit",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/server.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "tests/**",
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "axios": "^1.9.0",
    "express": "^4.21.2",
    "mongoose": "^7.8.7"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/express": "^4.17.22",
    "@types/mocha": "^10.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.17.47",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amilcarpio/technical-assessment-ozmap.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Amilcarpio/technical-assessment-ozmap/issues"
  },
  "homepage": "https://github.com/Amilcarpio/technical-assessment-ozmap#readme"
}
